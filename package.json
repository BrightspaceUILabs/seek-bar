{
  "name": "@d2l/seek-bar",
  "version": "2.1.0",
  "description": "Polymer media seek bar component",
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/d2l-seek-bar.git"
  },
  "homepage": "https://github.com/Brightspace/d2l-seek-bar",
  "scripts": {
    "lint": "npm run lint:wc && npm run lint:js",
    "lint:js": "eslint . --ext .js,.html test/**/*.js test/**/*.html demo/**/*.js demo/**/*.html",
    "lint:wc": "polymer lint",
    "start": "polymer serve -p 9998",
    "test": "npm run lint && npm run test:polymer:local",
    "test:polymer:local": "polymer test --skip-plugin sauce",
    "test:polymer:sauce": "polymer test --skip-plugin local"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/eslint-parser": "^7",
    "@babel/polyfill": "^7",
    "@polymer/iron-component-page": "^3.0.0-pre.18",
    "@polymer/iron-demo-helpers": "^3.0.0-pre.18",
    "@webcomponents/webcomponentsjs": "^2.1.3",
    "eslint": "^8",
    "eslint-config-brightspace": "^0.16",
    "eslint-plugin-html": "^6",
    "eslint-plugin-sort-class-members": "^1",
    "polymer-cli": "^1.9.0",
    "wct-browser-legacy": "^1.0.1",
    "web-component-tester": "^4.0.0"
  },
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  },
  "dependencies": {
    "@brightspace-ui/core": "^2",
    "@polymer/iron-a11y-keys-behavior": "^3.0.0-pre.18",
    "@polymer/iron-flex-layout": "^3.0.0-pre.18",
    "@polymer/iron-range-behavior": "^3.0.0-pre.18",
    "@polymer/polymer": "^3.0.0"
  }
}
